{
    "name": "XrayR",
    "description": "XrayR websocket proxy on heroku",
    "keywords": ["XrayR"],
    "env": {
        "PANELTYPE": {
            "description": "PANELTYPE",
            "value": "V2board"
        },
        "APIHOST": {
            "description": "APIHOST",
            "value": "https://api.v2board.com"
        },
        "APIKEY": {
            "description": "APIKEY",
            "value": "APIKEY"
        },
        "NODEID": {
            "description": "NODEID",
            "value": "1"
        },
        "CADDYIndexPage": {
            "description": "自定义CADDY部署完成后点击View的显示页面，参考：https://github.com/AkashiCoin/Xray-Heroku/blob/master/etc/CADDYIndexPage.md",
            "value": "https://github.com/AYJCSGM/mikutap/archive/master.zip"
        }
    },
    "website": "https://github.com/AkashiCoin/XrayR",
    "repository": "https://github.com/AkashiCoin/XrayR",
    "stack": "container"
}
